<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">
  
  <module package="com.aop.company.db" name="company"/>
  
  <entity name="Project">
	  <string name="name" />
	  <integer name="priority" />
	  <string name="projectRecordDate" />
	  <date name="prStDate" />
	  <date name="prEndDate"/>
	  <string name="notes" multiline="true" />
	  
	 <!-- M2O - many project one parent (this is done to show the main project also as its sub project) -->
		  	<many-to-one name="parent" ref="com.aop.company.db.Project"  />  
	    
	  <!-- M2M - many emp- many projects  -->
	  <many-to-many name="employeeSet" ref="com.aop.company.db.Employee" /> 
	  
		  <!-- O2M - one project - many project Task 	  -->
		  <one-to-many name="projT" ref="com.aop.company.db.ProjectTask"  mappedBy="projs" title="Tasks"/> 
		  
		  	 
  </entity>
</domain-models>