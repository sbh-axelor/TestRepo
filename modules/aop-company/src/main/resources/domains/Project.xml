<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">
  
  <module package="com.aop.company.db" name="company"/>
  
  <entity name="Project">
  
  <string name="projectName" />
  <string name="projectManager" />
  <string name="progress" />
  <date name="prStDate" />
  
  <!-- M2M - many emp- may projects -->
  <many-to-many ref="Employee" name="projEmp" required="true"/>
  
  <one-to-many name="projT" ref="ProjectTask"  mappedBy="projTasks"/> 
  
  
  </entity>
  </domain-models>