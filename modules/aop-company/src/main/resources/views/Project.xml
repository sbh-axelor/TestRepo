<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
  
  <grid model="com.aop.company.db.Project" title="Project Detail" name="proj-grid">
	  <field name="projectName" />
	  <field name="projectManager" />
	  <field name="prStDate" title="Project Start Date" />
  </grid>
  
  <form model="com.aop.company.db.Project" title="Project Form" name="proj-form">
  	<panel>
  	  <field name="projectName" />
	  <field name="projectManager" />
	  <field name="prStDate" title="Start Date"/>
	 </panel>
	  
	  <panel-related field="projEmp" name="projPanel">
	  	<field name="firstName"  />
		<field name="lastName" /> 
	  </panel-related>
	  
	  
  </form>
  
  
  
  </object-views>