<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">
  
  <module package="com.aop.company.db" name="company"/>
  
  <entity name="SubTask">
    <string name="name" title="Project Name" />
    <string name="task" title="Project Task" />
    <string name="subTask" title="Project Sub Task" />
    <string name="status"  /> 
    <integer name="priority" />
    <datetime name="startDate"/>
    <datetime name="endDate"/>
    <string name="testStatus" selection="test-selection"/>
    
    <many-to-one ref="com.aop.company.db.ProjectTask" name="subTasks" />  
  </entity>
  </domain-models>