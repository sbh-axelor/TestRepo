  	<form model="com.aop.company.db.Product" title="Product Form" name="product-form">
		<field name="name" title="Product Name" />
		<field name="code" title="Product Code" />
		<field name="purchaseDate" />
		<field name="cost"/> 	  	
  	</form>
  	
 </object-views>